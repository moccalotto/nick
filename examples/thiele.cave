init 72 x 72

suggest export.type  = iterm

# Print the pattern
thiele 71


## If we're only exporting to raw text, we won't do any scaling.
if $suggestion.export.type == text
    stop!
endif

# Scale up, remove outliers, and make sure all
# areas are accessible.
scale 2
prune-small-areas 20
connect-rooms 1

# Scale up with edge smoothing
loop 5
    scale 1.5
    evolve B5678/S5678
endloop
